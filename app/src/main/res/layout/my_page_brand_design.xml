<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="67dp"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<!--
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/my_page_brand_logo_image"
        android:layout_width="67dp"
        android:layout_height="67dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>-->
    <androidx.cardview.widget.CardView
        android:id="@+id/my_page_brand_logo_image_card"
        android:layout_width="67dp"
        android:layout_height="67dp"
        app:cardCornerRadius="33.5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
        <ImageView
            android:id="@+id/my_page_brand_logo_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitCenter"/>
    </androidx.cardview.widget.CardView>
    <TextView
        android:includeFontPadding="false"
        android:id="@+id/my_page_brand_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:textColor="#3e3a39"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/my_page_brand_logo_image_card"
        app:layout_constraintLeft_toLeftOf="@id/my_page_brand_logo_image_card"
        app:layout_constraintRight_toRightOf="@id/my_page_brand_logo_image_card"
        />
</androidx.constraintlayout.widget.ConstraintLayout>